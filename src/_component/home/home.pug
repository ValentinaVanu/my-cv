.home
//- we want home wrapper to be positioned
//- in the center vertically and horizontally
.home-wrapper
  h1.home-name #{resume.author.name}
  h2.home-title #{resume.author.title}
  ul
    li
      each social in resume.author.social
        a(href=social.url)
          span(class='resume-icon-'+social.icon)
